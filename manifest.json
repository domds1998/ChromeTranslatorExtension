{
    "name": "Language",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles/iconPopupStyle.css"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "images/icon_16x16.png",
        "default_popup": "popup/index.html",
        "default_title": "Lookup a definition!"
    },
    
    "web_accessible_resources": ["images/*"]
}